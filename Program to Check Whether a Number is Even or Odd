// using if-else

#include<iostream>
using namespace std;
int main()
{
int num;
cin>>num;
if(num%2==0)
cout<<"Even"<<endl;
else
cout<<"Odd";

return 0;
}


// using ternary operator

#include<iostream>
using namespace std;
int main()
{
int num;
cin>>num;

(num%2==0)?cout<<"Even";:cout<<"Odd";

return 0;
}

